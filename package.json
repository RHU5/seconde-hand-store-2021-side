{
  "name": "shstore",
  "version": "1.0.0",
  "description": "Second-hand store Side Project",
  "main": "init.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 1",
    "build:server": "babel src --out-dir build --ignore 'src/assets' --ignore 'src/static' --ignore 'src/webpack.config.js'",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "prebuild": "rm -rf build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prestart":"npm run build",
    "start": "PRODUCTION=true node build/init.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RHU5/seconde-hand-store-2021-side.git"
  },
  "author": "rhu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RHU5/seconde-hand-store-2021-side/issues"
  },
  "homepage": "https://github.com/RHU5/seconde-hand-store-2021-side#readme",
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "autoprefixer": "^10.2.5",
    "aws-sdk": "^2.864.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "^1.4.5",
    "css-loader": "^5.1.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "mini-css-extract-plugin": "^1.3.9",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-google-oauth2": "^0.2.0",
    "passport-kakao": "^1.0.1",
    "passport-local-mongoose": "^6.1.0",
    "passport-naver": "^1.0.6",
    "postcss-loader": "^5.2.0",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "2.2.1"
  }
}
